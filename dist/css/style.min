body{padding-top:70px}.navbar-inverse .navbar-brand:hover,.navbar-inverse .navbar-nav>li>a:hover{color:#f17721}.navbar-inverse .navbar-toggle .icon-bar{background-color:#f17721}img{box-shadow:0 5px 8px rgba(0,0,0,.5);height:300px;margin-bottom:10px;width:700px}footer,footer .text-center{width:100%}h3{margin-top:0}h5{font-size:24px}.btn-primary{background-color:#219bf1}.btn-primary:hover{background-color:#077ed2}ul.tech{list-style:none;padding:0}ul.tech li{background-color:#f17721;border-radius:5px;color:#fff;display:inline-flex;margin-top:5px;padding:5px}ul.tech li.js{background-color:#929487}ul.tech li.api{background-color:#89CEDE}ul.tech li.other{background-color:#C7C9BE}ul.pagination li a{color:#f17721;border-color:#9d9d9d}ul.pagination li a:hover{color:#000;border-color:#9d9d9d}ul.pagination li.active a{background-color:#f17721;color:#000}ul.pagination li.active a:hover{background-color:#f17721}footer{color:#fff;left:0;position:absolute;right:0}footer .footer-above{background-color:#222;margin-top:20px}footer .footer-col{margin-bottom:50px}footer .footer-below{background-color:#222;padding:25px 0}.btn-social{color:#fff;border-radius:100%;border:2px solid #fff;display:inline-block;font-size:20px;height:50px;line-height:45px;text-align:center;width:50px}.btn-outline.active,.btn-outline:active,.btn-outline:focus,.btn-outline:hover{background:#9d9d9d;border:2px solid #f17721;color:#fff}@media only screen and (min-device-width :414px) and (max-device-width :736px) and (orientation :portrait) and (-webkit-min-device-pixel-ratio:2){h1{font-size:40px!important}}